    "account",
    "acl"
  ],
  "author": "ksmithut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithut/mongoose-role/issues"
  },
  "homepage": "https://github.com/ksmithut/mongoose-role",
  "devDependencies": {
    "codeclimate-test-reporter": "0.3.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.2",
    "jshint": "^2.6.0",
    "jshint-stylish": "^2.1.0",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "1.0.0",
    "mongoose": "^4.3.6"
  },
  "dependencies": {
    "defaults": "^1.0.0",
    "lodash.defaults": "^4.0.0"
  }
